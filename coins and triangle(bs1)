#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int count=0;int sum=0;
	    int i=0;
	    
	    while(n>sum){
	        i++;
	        sum+=i;
	        count++;
	    }
	    if(sum==n){
	        cout<<count<<endl;
	        
	    }
	    else{
	    cout<<count-1<<endl;
	    }
	    
	
	}
	return 0;
}
