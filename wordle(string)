#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    string s,t;
	    cin>>s>>t;
	    for(int  i=0;i<5;i++){
	        if(s[i]!=t[i])
	        cout<<'B';
	        else
	        cout<<'G';
	        
	    }
	    cout<<endl;
	    
	    
	}
	return 0;
}
