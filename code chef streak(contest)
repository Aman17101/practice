#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t, n;
	cin>>t;
	while(t--){
	    cin>>n;
	    int a[n];
	    int o[n];
	    int x=0;
	    int y=0;
	    int w=0;
	    int z=0;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++){
	        cin>>o[i];
	    }
	    for(int i=0;i<n;i++){
	        if(a[i]==0){
	            x=0;
	        }
	        else {
	            x++;
	        }
	if(x>w){
	    w=x;
	}
	    }
	    
	    for(int i=0;i<n;i++){
	        if(o[i]==0){
	            y=0;
	        }
	        else {
	            y++;
	        }
	        if(y>z){
	            z=y;
	        }
	        	    }
	    if(w>z){
	        cout<<"om"<<endl;
	    }
	    else if(z>w){
	        cout<<"addy"<<endl;
	    }
	    else {
	        cout<<"draw"<<endl;
	    }
	}
	
	return 0;
}
